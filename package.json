    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",

    "csvtojson": "^2.0.10",
    "dotenv": "^10.0.0",
    "eslint-plugin-jest": "^24.4.2",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "express-validator": "^6.12.1",
    "firebase-admin": "^9.11.1",
    "generate-password": "^1.6.1",
